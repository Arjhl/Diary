generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url = env("DATABASE_URL")
  relationMode = "prisma"
}
  
model User {
    id String @id
    email String @unique
    username String?
}

model Diaries {
    id Int @id @default(autoincrement())
    user_id String
    title String
    diary String
    createdAt DateTime @default(now())
}